{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "ibl_sessions.schema.json", "additionalProperties": false,
  "properties": {
    "nickname": {"type": ["string","null"]},
    "url": {"type": ["string","null"]},
    "id": {"type": ["boolean","string"]},
    "responsible_user": {"type": ["string","null"]},
    "birth_date": {"type": ["string","null"]},
    "age_weeks": {"type": ["number","boolean"]},
    "death_date": {"type": ["string","null"]},
    "species": {"type": ["string","null"]},
    "sex": {"type": ["string","null"], "enum": ["M", "F", null]},
    "litter": {"type": ["string","null"]},
    "strain": {"type": ["string","null"]},
    "source": {"type": ["string","null"]},
    "line": {"type": ["string","null"]},
    "projects": {"type": "array", "items": {"type": "string"}},
    "session_projects": {"type": "array", "items": {"type": "string"}},
    "lab": {"type": ["string","null"]},
    "genotype": {"type": "array"},
    "description": {"type": ["string","null"]},
    "alive": {"type": ["boolean"]},
    "reference_weight": {"type": ["number","boolean"]},
    "last_water_restriction": {"type": ["string","null"]},
    "expected_water": {"type": ["number","boolean"]},
    "remaining_water": {"type": ["number","boolean"]},
    "weighings": {"type": "array", "items": {"type": "object"}},
    "water_administrations": {"type": "array", "items": {"type": "object"}}
  }
}